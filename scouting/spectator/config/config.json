{
  "$schema": "./schema.json",
  "title": "QRScout",
  "page_title": "Crescendo",
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": true,
      "fields": [
        {
          "title": "Scouter Initials",
          "type": "text",
          "required": true,
          "code": "scouter"
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "preserveDataOnReset": true,
          "autoIncrementOnReset": true
        },
        {
          "title": "Robot",
          "type": "select",
          "required": true,
          "code": "robot",
          "choices": {
            "R1": "Red 1",
            "R2": "Red 2",
            "R3": "Red 3",
            "B1": "Blue 1",
            "B2": "Blue 2",
            "B3": "Blue 3"
          },
          "defaultValue": "R1"
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber"
        },
        {
          "title": "Starting Position",
          "type": "select",
          "required": true,
          "code": "startPosition",
          "choices": {
            "1": "Source",
            "2": "Middle",
            "3": "Amp"
          },
          "defaultValue": ""
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "isNoShow"
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Moved",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "hasMoved"
        },
        {
          "code": "autoSpeakerScoredCount",
          "title": "Speaker Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "autoSpeakerMissedCount",
          "title": "Speaker Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "autoGamePiecePath",
          "title": "Auto Game Piece Path",
          "type": "select",
          "multiSelect": true,
          "choices": {
            "1": "Speaker 3 (Source Side)",
            "2": "Speaker 2 (Middle)",
            "3": "Speaker 1 (Amp Side)",
            "4": "Midline 5 (Source Edge)",
            "5": "Midline 4",
            "6": "Midline 3 (Middle)",
            "7": "Midline 2",
            "8": "Midline 1 (Amp Edge)"
          }
        },
        {
          "code": "autoFoulCount",
          "title": "Auto Foul",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "ampScoredCount",
          "title": "Amp Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "speakerScoredCount",
          "title": "Speaker Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "feederShotsCount",
          "title": "Feeder Shots",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "noteInTrapCount",
          "title": "Notes in Trap",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false
        },
        {
          "code": "teleopFoulCount",
          "title": "Teleop Foul",
          "type": "counter",
          "defaultValue": 0,
          "required": false
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "End Position",
          "type": "select",
          "required": true,
          "code": "endPosition",
          "choices": {
            "0": "No Climb",
            "1": "Parked",
            "2": "Onstage",
            "4": "Harmony",
            "3": "Failed Harmony"
          },
          "defaultValue": "No"
        },
        {
          "code": "offenseSkillRating",
          "title": "Offense Skill",
          "type": "select",
          "choices": {
            "1": "Not Effective",
            "2": "Average",
            "3": "Very Effective",
            "x": "Not Observed"
          },
          "defaultValue": "x",
          "required": false
        },
        {
          "code": "defenseSkillRating",
          "title": "Defense Skill",
          "type": "select",
          "choices": {
            "1": "Not Effective",
            "2": "Average",
            "3": "Very Effective",
            "x": "Not Observed"
          },
          "defaultValue": "x",
          "required": false
        },
        {
          "code": "hasDiedOrTippedOver",
          "title": "Died / Tipped Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false
        },
        {
          "code": "yellowRedCard",
          "title": "Yellow/Red Card",
          "type": "select",
          "defaultValue": "No Card",
          "required": true,
          "choices": {
            "None": "No Card",
            "Yellow": "Yellow Card",
            "Red": "Red Card"
          }
        },
        {
          "code": "comments",
          "title": "Comments",
          "type": "markdown",
          "min": 0,
          "max": 50,
          "required": false
        }
      ]
    }
  ]
}
