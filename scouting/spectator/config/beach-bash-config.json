{
  "$schema": "./schema.json",
  "title": "Spectator",
  "page_title": "Beach Bash",
  "aggregateBy": "teamNumber",
  "distinct": ["scouter", "matchNumber"],
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": true,
      "fields": [
        {
          "title": "Scouter Initials",
          "type": "text",
          "required": true,
          "code": "scouter",
          "columnOrder": 2
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "preserveDataOnReset": true,
          "autoIncrementOnReset": true,
          "columnOrder": 0
        },
        {
          "title": "Robot",
          "type": "enum",
          "required": true,
          "code": "robot",
          "choices": [
            "Red 1",
            "Red 2",
            "Blue 1",
            "Blue 2"
          ],
          "defaultValue": "0",
          "columnOrder": 3
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "columnOrder": 1
        },
        {
          "title": "Starting Position",
          "type": "enum",
          "required": true,
          "code": "startPosition",
          "choices": [
            "Top Right",
            "Top Left",
            "Bottom Right",
            "Bottom Left"
          ],
          "columnOrder": 4
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "isNoShow",
          "columnOrder": 5
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Moved",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "hasMoved",
          "columnOrder": 6
        },
        {
          "code": "autoNetScoredCount",
          "title": "Net Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 7
        },
        {
          "code": "autoNetMissedCount",
          "title": "Net Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 8
        },
        {
          "code": "autoHoopScoredCount",
          "title": "Hoop Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 9
        },
        {
          "code": "autoHoopMissedCount",
          "title": "Hoop Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 10
        },
        {
          "code": "autoFoulCount",
          "title": "Auto Foul",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 11
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "netScoredCount",
          "title": "Net Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 12
        },
        {
          "code": "hoopScoredCount",
          "title": "Hoop Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 13
        },
        {
          "code": "teleopFoulCount",
          "title": "Foul",
          "type": "counter",
          "defaultValue": 0,
          "required": false,
          "columnOrder": 14
        },
        {
          "code": "teleopTechFoulCount",
          "title": "Tech Foul",
          "type": "counter",
          "defaultValue": 0,
          "required": false,
          "columnOrder": 15
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "End Position",
          "type": "enum",
          "required": true,
          "code": "endPosition",
          "choices": [
            "Not Home",
            "Home"
          ],
          "defaultValue": "No",
          "columnOrder": 16
        },
        {
          "code": "offenseSkillRating",
          "title": "Offense Skill",
          "type": "enum",
          "choices": [
            "Not Observed",
            "Not Effective",
            "Average",
            "Very Effective"
          ],
          "required": false,
          "columnOrder": 17
        },
        {
          "code": "defenseSkillRating",
          "title": "Defense Skill",
          "type": "enum",
          "choices": [
            "Not Observed",
            "Not Effective",
            "Average",
            "Very Effective"
          ],
          "required": false,
          "columnOrder": 18
        },
        {
          "code": "hasDiedOrTippedOver",
          "title": "Died / Tipped Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "columnOrder": 19
        },
        {
          "code": "yellowRedCard",
          "title": "Yellow/Red Card",
          "type": "enum",
          "defaultValue": "No Card",
          "required": true,
          "choices": [
            "No Card",
            "Yellow Card",
            "Red Card"
          ],
          "columnOrder": 20
        },
        {
          "code": "comments",
          "title": "Comments",
          "type": "markdown",
          "min": 0,
          "max": 50,
          "required": false,
          "columnOrder": 21
        }
      ]
    }
  ]
}
