{
  "$schema": "./schema.json",
  "title": "Spectator",
  "page_title": "Crescendo",
  "aggregateBy": "teamNumber",
  "distinct": ["scouter", "matchNumber"],
  "sections": [
    {
      "name": "Prematch",
      "preserveDataOnReset": true,
      "fields": [
        {
          "title": "Scouter Initials",
          "type": "text",
          "required": true,
          "code": "scouter",
          "columnOrder": 2
        },
        {
          "title": "Match Number",
          "type": "number",
          "required": true,
          "code": "matchNumber",
          "preserveDataOnReset": true,
          "autoIncrementOnReset": true,
          "columnOrder": 0
        },
        {
          "title": "Robot",
          "type": "enum",
          "required": true,
          "code": "robot",
          "choices": [
            "Red 1",
            "Red 2",
            "Red 3",
            "Blue 1",
            "Blue 2",
            "Blue 3"
          ],
          "defaultValue": "0",
          "columnOrder": 3
        },
        {
          "title": "Team Number",
          "type": "number",
          "required": true,
          "code": "teamNumber",
          "columnOrder": 1
        },
        {
          "title": "Starting Position",
          "type": "enum",
          "required": true,
          "code": "startPosition",
          "choices": [
            "Source",
            "Middle",
            "Amp"
          ],
          "columnOrder": 4
        },
        {
          "title": "No Show",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "isNoShow",
          "columnOrder": 5,
          "aggregate": [{
            "title": "No Show Count",
            "code": "noShowCount",
            "aggFunction": "countTrue"
          }]
        }
      ]
    },
    {
      "name": "Autonomous",
      "fields": [
        {
          "title": "Moved",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "code": "hasMoved",
          "columnOrder": 6,
          "aggregate": [{
            "title": "Moved Count",
            "code": "movedCount",
            "aggFunction": "countTrue"
          }]
        },
        {
          "code": "autoSpeakerScoredCount",
          "title": "Speaker Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 7,
          "aggregate": [{
            "title": "Speaker Scored Minimum",
            "code": "autoSpeakerScoredMin",
            "aggFunction": "min"
          },{
            "title": "Speaker Scored Maximum",
            "code": "autoSpeakerScoredMax",
            "aggFunction": "max"
          },{
            "title": "Speaker Scored Median",
            "code": "autoSpeakerScoredMedian",
            "aggFunction": "median"
          },{
            "title": "Speaker Scored Mean",
            "code": "autoSpeakerScoredMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "autoSpeakerMissedCount",
          "title": "Speaker Missed",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 8,
          "aggregate": [{
            "title": "Speaker Missed Minimum",
            "code": "autoSpeakerMissedMin",
            "aggFunction": "min"
          },{
            "title": "Speaker Missed Maximum",
            "code": "autoSpeakerMissedMax",
            "aggFunction": "max"
          },{
            "title": "Speaker Missed Median",
            "code": "autoSpeakerMissedMedian",
            "aggFunction": "median"
          },{
            "title": "Speaker Missed Mean",
            "code": "autoSpeakerMissedMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "autoGamePiecePath",
          "title": "Game Piece Path",
          "type": "enum-set",
          "choices": [
            "Speaker 3 (Source Side)",
            "Speaker 2 (Middle)",
            "Speaker 1 (Amp Side)",
            "Midline 5 (Source Edge)",
            "Midline 4",
            "Midline 3 (Middle)",
            "Midline 2",
            "Midline 1 (Amp Edge)"
          ],
          "required": false,
          "defaultValue": 0,
          "columnOrder": 9,
          "aggregate": [{
            "title": "Game Piece Path Union Count",
            "code": "autoGamePiecePathUnionCount",
            "aggFunction": "unionCount"
          },{
            "title": "Game Piece Path intersection Count",
            "code": "autoGamePiecePathIntersectionCount",
            "aggFunction": "intersectionCount"
          },{
            "title": "Game Piece Path Count Minimum",
            "code": "autoGamePiecePathCountMin",
            "aggFunction": "minCount"
          },{
            "title": "Game Piece Path Count Maximum",
            "code": "autoGamePiecePathCountMax",
            "aggFunction": "maxCount"
          }]
        },
        {
          "code": "autoFoulCount",
          "title": "Fouls",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 10,
          "aggregate": [{
            "title": "Foul Count Minimum",
            "code": "autoFoulCountMin",
            "aggFunction": "min"
          },{
            "title": "Foul Count Maximum",
            "code": "autoFoulCountMax",
            "aggFunction": "max"
          },{
            "title": "Foul Count Median",
            "code": "autoFoulCountMedian",
            "aggFunction": "median"
          },{
            "title": "Foul Count Mean",
            "code": "autoFoulCountMean",
            "aggFunction": "mean"
          }]
        }
      ]
    },
    {
      "name": "Teleop",
      "fields": [
        {
          "code": "ampScoredCount",
          "title": "Amp Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 11,
          "aggregate": [{
            "title": "Amp Scored Minimum",
            "code": "ampScoredMin",
            "aggFunction": "min"
          },{
            "title": "Amp Scored Maximum",
            "code": "ampScoredMax",
            "aggFunction": "max"
          },{
            "title": "Amp Scored Median",
            "code": "ampScoredMedian",
            "aggFunction": "median"
          },{
            "title": "Amp Scored Mean",
            "code": "ampScoredMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "speakerScoredCount",
          "title": "Speaker Scored",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 12,
          "aggregate": [{
            "title": "Speaker Scored Minimum",
            "code": "speakerScoredMin",
            "aggFunction": "min"
          },{
            "title": "Speaker Scored Maximum",
            "code": "speakerScoredMax",
            "aggFunction": "max"
          },{
            "title": "Speaker Scored Median",
            "code": "speakerScoredMedian",
            "aggFunction": "median"
          },{
            "title": "Speaker Scored Mean",
            "code": "speakerScoredMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "feederShotsCount",
          "title": "Feeder Shots",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 13,
          "aggregate": [{
            "title": "Feeder Shots Minimum",
            "code": "feederShotsMin",
            "aggFunction": "min"
          },{
            "title": "Feeder Shots Maximum",
            "code": "feederShotsMax",
            "aggFunction": "max"
          },{
            "title": "Feeder Shots Median",
            "code": "feederShotsMedian",
            "aggFunction": "median"
          },{
            "title": "Feeder Shots Mean",
            "code": "feederShotsMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "noteInTrapCount",
          "title": "Notes in Trap",
          "type": "counter",
          "defaultValue": 0,
          "min": 0,
          "required": false,
          "columnOrder": 14,
          "aggregate": [{
            "title": "Notes In Trap Minimum",
            "code": "notesInTrapMin",
            "aggFunction": "min"
          },{
            "title": "Notes In Trap Maximum",
            "code": "notesInTrapMax",
            "aggFunction": "max"
          },{
            "title": "Notes In Trap Median",
            "code": "notesInTrapMedian",
            "aggFunction": "median"
          },{
            "title": "Notes In Trap Mean",
            "code": "notesInTrapMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "teleopFoulCount",
          "title": "Fouls",
          "type": "counter",
          "defaultValue": 0,
          "required": false,
          "columnOrder": 15,
          "aggregate": [{
            "title": "Fouls Minimum",
            "code": "foulsMin",
            "aggFunction": "min"
          },{
            "title": "Fouls Maximum",
            "code": "foulsMax",
            "aggFunction": "max"
          },{
            "title": "Fouls Median",
            "code": "foulsMedian",
            "aggFunction": "median"
          },{
            "title": "Fouls Mean",
            "code": "foulsMean",
            "aggFunction": "mean"
          }]
        }
      ]
    },
    {
      "name": "Postmatch",
      "fields": [
        {
          "title": "End Position",
          "type": "enum",
          "required": true,
          "code": "endPosition",
          "choices": [
            "No Climb",
            "Parked",
            "Onstage",
            "Failed Harmony",
            "Harmony"
          ],
          "defaultValue": "No",
          "columnOrder": 16,
          "aggregate": [{
            "title": "End Position Minimum",
            "code": "endPositionMin",
            "aggFunction": "min"
          },{
            "title": "End Position Maximum",
            "code": "endPositionMax",
            "aggFunction": "max"
          },{
            "title": "End Position Median",
            "code": "endPositionMedian",
            "aggFunction": "median"
          },{
            "title": "End Position Mean",
            "code": "endPositionMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "offenseSkillRating",
          "title": "Offense Skill",
          "type": "enum",
          "choices": [
            "Not Effective",
            "Average",
            "Very Effective",
            "Not Observed"
          ],
          "defaultValue": "x",
          "required": false,
          "columnOrder": 17,
          "aggregate": [{
            "title": "Offense Skill Rating Minimum",
            "code": "offenseSkillRatingMin",
            "aggFunction": "min"
          },{
            "title": "Offense Skill Rating Maximum",
            "code": "offenseSkillRatingMax",
            "aggFunction": "max"
          },{
            "title": "Offense Skill Rating Median",
            "code": "offenseSkillRatingMedian",
            "aggFunction": "median"
          },{
            "title": "Offense Skill Rating Mean",
            "code": "offenseSkillRatingMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "defenseSkillRating",
          "title": "Defense Skill",
          "type": "enum",
          "choices": [
            "Not Effective",
            "Average",
            "Very Effective",
            "Not Observed"
          ],
          "defaultValue": "x",
          "required": false,
          "columnOrder": 18,
          "aggregate": [{
            "title": "Defense Skill Rating Minimum",
            "code": "defenseSkillRatingMin",
            "aggFunction": "min"
          },{
            "title": "Defense Skill Rating Maximum",
            "code": "defenseSkillRatingMax",
            "aggFunction": "max"
          },{
            "title": "Defense Skill Rating Median",
            "code": "defenseSkillRatingMedian",
            "aggFunction": "median"
          },{
            "title": "Defense Skill Rating Mean",
            "code": "defenseSkillRatingMean",
            "aggFunction": "mean"
          }]
        },
        {
          "code": "hasDiedOrTippedOver",
          "title": "Died / Tipped Over",
          "type": "boolean",
          "defaultValue": false,
          "required": false,
          "columnOrder": 19,
          "aggregate": [{
            "title": "Died / Tipped Over Count",
            "code": "hasDiedOrTippedOverCount",
            "aggFunction": "countTrue"
          }]
        },
        {
          "code": "yellowRedCard",
          "title": "Yellow/Red Card",
          "type": "enum",
          "defaultValue": "No Card",
          "required": true,
          "choices": [
            "No Card",
            "Yellow Card",
            "Red Card"
          ],
          "columnOrder": 20,
          "aggregate": [{
            "title": "Any Card Count",
            "code": "anyCardCount",
            "aggFunction": "countIfNot",
            "additionalArguments": [0]
          },{
            "title": "Yellow Card Count",
            "code": "yellowCardCount",
            "aggFunction": "countIf",
            "additionalArguments": [1]
          },{
            "title": "Red Card Count",
            "code": "redCardCount",
            "aggFunction": "countIf",
            "additionalArguments": [2]
          }]
        },
        {
          "code": "comments",
          "title": "Comments",
          "type": "markdown",
          "min": 0,
          "max": 50,
          "required": false,
          "columnOrder": 21
        }
      ]
    }
  ]
}
